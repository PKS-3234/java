import java.util.*;
class Main{
     int fact(int n){
       if(n==0 || n==1)
        return 1;
       
       return (n*fact(n-1));
    }
    
    public static void main(String[] args){
        Scanner sc=new Scanner (System.in);
        int n= sc.nextInt();
        Main obj=new Main();
        int res=obj.fact(n);
        System.out.println(res);
        
    }
}
