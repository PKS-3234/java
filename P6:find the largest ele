import java.util.*;
class Demo{
    public static void main(String[] args)
    {  
        Scanner sc= new Scanner(System.in);
        int n= sc.nextInt();
        int[] array = new int[n];
        for(int i=0;i<n;i++)
        {
            array[i]=sc.nextInt();
            
        }
        if(n==1)
        System.out.println(array[0]);
        else
        {
            Arrays.sort(array);
            System.out.println(array[n-1]);
        }
        
        
    }
}
